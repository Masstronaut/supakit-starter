{
	"name": "supakit-starter",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"supabase:typegen": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/lib/types/supabase.ts",
		".env": "dotenv-run-script .env --",
		"db:init": "prisma db push",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^7.3.0",
		"@storybook/addon-interactions": "^7.3.0",
		"@storybook/addon-links": "^7.3.0",
		"@storybook/addon-svelte-csf": "^3.0.7",
		"@storybook/blocks": "^7.3.0",
		"@storybook/svelte": "^7.3.0",
		"@storybook/sveltekit": "^7.3.0",
		"@storybook/testing-library": "^0.0.14-next.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"daisyui": "3.0.0",
		"dotenv-run-script": "^0.2.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-storybook": "^0.6.12",
		"eslint-plugin-svelte": "^2.32.4",
		"postcss": "^8.4.23",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"prettier-plugin-tailwindcss": "^0.2.7",
		"prisma": "^5.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.3.0",
		"supabase": "^1.29.3",
		"svelte": "^4.1.2",
		"svelte-check": "3.4.3",
		"svelte-french-toast": "^1.0.3",
		"svelte-preprocess": "5.0.4",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.4",
		"vite": "4.3.9"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^3.0.1",
		"@prisma/client": "^5.1.1",
		"@steeze-ui/heroicons": "^2.2.1",
		"@steeze-ui/svelte-icon": "^1.3.2",
		"lucia": "^2.3.0",
		"zod": "^3.21.4"
	}
}
